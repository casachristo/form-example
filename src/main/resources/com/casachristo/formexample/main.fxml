<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="700.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="80.0" prefWidth="580.0">
         <children>
         
             <Label fx:id="lblHeading" prefHeight="80.0" style="-fx-font-weight: bold;" text="Student Form" wrapText="true" />
         </children>
      </HBox>
      <GridPane prefHeight="500.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="400.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="First Name:" />
            <Label text="Last Name:" GridPane.rowIndex="1" />
            <Label text="DoB:" GridPane.rowIndex="2" />
            <Label text="Address:" GridPane.rowIndex="3" />
            <Label text="City:" GridPane.rowIndex="4" />
            <Label text="State:" GridPane.rowIndex="5" />
            <Label text="Phone:" GridPane.rowIndex="6" />
            <Label text="Email:" GridPane.rowIndex="7" />
            <Label text="Major:" GridPane.rowIndex="8" />
            <Label text="Date:" GridPane.rowIndex="9" />
            <TextField fx:id="txtFirstName" GridPane.columnIndex="1" />
            <TextField fx:id="txtLastName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtAddress" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="txtCity" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="txtState" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="txtPhone" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextField fx:id="txtEmail" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <DatePicker fx:id="dateDate" prefHeight="39.0" prefWidth="472.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
            <ComboBox fx:id="cbMajor" prefHeight="39.0" prefWidth="448.0" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <DatePicker fx:id="dateDOB" prefHeight="39.0" prefWidth="463.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <HBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="600.0">
         <children>
             <Button fx:id="btnSubmit" onAction="#onSubmitButtonClick" text="Submit" />
         </children>
      </HBox>
   </children>
</VBox>
